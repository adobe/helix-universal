{
  "name": "@adobe/helix-universal",
  "version": "5.0.5",
  "description": "Helix Universal",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./google": "./src/google-adapter.js",
    "./aws": "./src/aws-adapter.js",
    "./openwhisk": "./src/openwhisk-adapter.js"
  },
  "scripts": {
    "test": "c8 mocha --spec=test/**/*.test.js",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-universal"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-universal/issues"
  },
  "mocha": {
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json",
    "loader": "esmock",
    "require": [
      "test/setup-env.js",
      "mocha-suppress-logs"
    ]
  },
  "homepage": "https://github.com/adobe/helix-universal#readme",
  "dependencies": {
    "@adobe/fetch": "4.1.8",
    "aws4": "1.13.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.6",
    "@google-cloud/secret-manager": "5.6.0",
    "@google-cloud/storage": "7.12.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "aws-sdk": "2.1667.0",
    "c8": "10.1.2",
    "eslint": "8.57.0",
    "esmock": "2.6.7",
    "husky": "9.1.4",
    "junit-report-builder": "4.0.1",
    "lint-staged": "15.2.8",
    "mocha": "10.7.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-suppress-logs": "0.5.1",
    "nock": "13.5.4",
    "semantic-release": "24.0.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
