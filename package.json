{
  "name": "@adobe/helix-universal",
  "version": "4.4.0",
  "description": "Helix Universal",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./google": "./src/google-adapter.js",
    "./aws": "./src/aws-adapter.js",
    "./openwhisk": "./src/openwhisk-adapter.js"
  },
  "scripts": {
    "test": "c8 mocha --spec=test/**/*.test.js",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-universal"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-universal/issues"
  },
  "mocha": {
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json",
    "loader": "esmock",
    "require": [
      "test/setup-env.js"
    ]
  },
  "homepage": "https://github.com/adobe/helix-universal#readme",
  "dependencies": {
    "@adobe/fetch": "4.1.0",
    "aws4": "1.12.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.3",
    "@google-cloud/secret-manager": "5.0.1",
    "@google-cloud/storage": "7.3.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "aws-sdk": "2.1473.0",
    "c8": "8.0.1",
    "eslint": "8.51.0",
    "esmock": "2.5.4",
    "husky": "8.0.3",
    "junit-report-builder": "3.1.0",
    "lint-staged": "15.0.1",
    "mocha": "10.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "nock": "13.3.4",
    "semantic-release": "22.0.5"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
